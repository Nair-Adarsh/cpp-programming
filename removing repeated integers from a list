#include<bits/stdc++.h>
using namespace std;
int main()
{
	int n,arr[100];
	cout<<"Enter the number of elements "<<endl;
	cin>>n;
	cout<<"Enter the elements"<<endl;
	for(int i=0;i<n;i++)
	{
		cin>>arr[i];
	}
	for(int i=0;i<n;i++)
	{
		int Flag=0;
		for(int j=0;j<i;j++)
		{
			if(arr[i]==arr[j])
			{
				Flag++;
			}
		}
		if(Flag==0)
		{
			cout<<arr[i]<<" ";
		}
	}
	cout<<endl;
	return main();
}

