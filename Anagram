//Anagram checking

#include<bits/stdc++.h>
#include<string.h>
using namespace std;
int main()
{
	string first,second;
	cout<<"Enter two words"<<endl;
	cin>>first>>second;
	int len1=first.size();
	int len2=second.size();
	if(len1!=len2)
	{
		cout<<"Anagram is not possible"<<endl;
		return main();
	}
	else{
		int len=len1;
		sort(first.begin(),first.end());
		sort(second.begin(),second.end());
		if(first==second)
		{
			cout<<"Anagram"<<endl;
		}
		else{
			cout<<"Not anagram"<<endl;
		}
	}
	return main();
}

